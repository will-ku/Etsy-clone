<!DOCTYPE html>
<html>
  <head>
    <%= favicon_link_tag asset_path('pixsy-favicon.png')%>
    <title>Pixsy</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
    <script type="text/javascript">
      window.pixsylogoURL = "<%= image_url('pixsy-logo-png.png') %>";
    </script>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
